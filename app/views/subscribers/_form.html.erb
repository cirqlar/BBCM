<%= form_for @subscriber, remote: true do |f| %>
  <h3>Get the latest info</h3>
  <p>
    Subscribe now to have announcements emailed to you once they are made
  </p>
  <div class="cf" id="create">

  </div>

  <%= render 'shared/errors', object: f.object %>

  <fieldset>
    <%= f.label :name %>
    <%= f.text_field :name%>
  </fieldset>

  <fieldset>
    <%= f.label :email %>
    <%= f.text_field :email %>
  </fieldset>

  <%= f.submit 'Subscribe' %>
<% end %>
