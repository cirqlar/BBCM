<%= form_for(@image, html: {multipart: true}) do |f| %>
  <%= render 'shared/errors', object: f.object %>

  <%= f.label :event %>
  <%= f.text_field :event %>

  <%= f.label :people %>
  <%= f.text_field :people %>

  <%= f.label :image %>
  <%= f.file_field :image %>

  <%= f.submit "Save Image" %>
<% end %>
