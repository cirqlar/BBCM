<div class="admin">
  <h3><%= admin.name %></h3>
  <h4><%= admin.email %></h4>
  <h5><%= link_to "Edit", edit_admin_path(admin), class: "delete" if logged_in? && correct_admin?(admin) %>  <%= link_to "Delete", admin, method: :delete, class: "delete", data: {confirm: "Are you sure?"} if logged_in? && correct_admin?(admin) %></h5>
</div>
