<%= form_for(@an) do |f| %>
  <%= render 'shared/errors', object: f.object %>

  <%= f.label :title %>
  <%= f.text_field :title %>

  <%= f.label :desc %>
  <%= f.text_area :desc %>

  <%= f.label :expires_at %>
  <%= f.datetime_select :expires_at %>

  <%= f.submit "Save Announcement" %>
<% end %>
