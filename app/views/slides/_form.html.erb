<%= form_for(@slide, html: {multipart: true}) do |f| %>
  <%= render 'shared/errors', object: f.object %>

  <%= f.label :image %>
  <%= f.file_field :image%>

  <%= f.label :title %>
  <%= f.text_field :title %>

  <%= f.label :desc %>
  <%= f.text_area :desc %>

  <%= f.submit 'Save Slide' %>
<% end %>
