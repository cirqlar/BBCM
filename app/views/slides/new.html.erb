<%= form_for(@slide, html: {multipart: true}) do |f| %>
  <%= render 'shared/form_top' %>

  <%= render 'shared/errors', object: f.object %>

  <fieldset>
    <%= f.label :image %>
    <%= f.file_field :image%>
  </fieldset>

  <fieldset>
    <%= f.label :title %>
    <%= f.text_field :title %>
  </fieldset>

  <%= f.submit 'Save Slide' %>
<% end %>
